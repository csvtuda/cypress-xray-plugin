{
    "name": "@csvtuda/cypress-xray-plugin",
    "version": "8.6.4",
    "description": "A Cypress plugin for uploading test results to Xray (test management for Jira)",
    "types": "index.d.ts",
    "author": "csvtuda",
    "repository": {
        "type": "git",
        "url": "https://github.com/csvtuda/cypress-xray-plugin"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/csvtuda/cypress-xray-plugin/issues"
    },
    "homepage": "https://csvtuda.github.io/docs/cypress-xray-plugin/",
    "keywords": [
        "cypress",
        "xray",
        "upload",
        "screenshot",
        "video",
        "cucumber"
    ],
    "files": [
        "src",
        "*.js",
        "*.ts"
    ],
    "exports": {
        ".": "./index.js",
        "./commands": "./src/tasks/commands.js",
        "./commands/tasks": "./src/tasks/index.js"
    },
    "directories": {
        "lib": "src"
    },
    "scripts": {
        "test": "node --import tsx ./test/run-unit-tests.ts",
        "test:coverage": "npx shx mkdir -p coverage && c8 --config .c8.json npm run test",
        "test:integration": "node --import tsx ./test/run-integration-tests.ts",
        "test:server": "node --import tsx ./test/run-server.ts",
        "build": "node --import tsx build.ts",
        "eslint": "eslint src/**/*.ts test/**/*.ts index.ts"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "@cucumber/gherkin": "^37.0.1",
        "@cucumber/messages": "^31.1.0",
        "ansi-colors": "^4.1.3",
        "axios": "^1.13.4",
        "form-data": "^4.0.5",
        "semver": "^7.7.3"
    },
    "peerDependencies": {
        "@badeball/cypress-cucumber-preprocessor": ">=17",
        "cypress": ">=10.0.0 < 16"
    },
    "peerDependenciesMeta": {
        "@badeball/cypress-cucumber-preprocessor": {
            "optional": true
        }
    },
    "devDependencies": {
        "@badeball/cypress-cucumber-preprocessor": "^24.0.0",
        "@bahmutov/cypress-esbuild-preprocessor": "^2.2.7",
        "@eslint/js": "^9.39.2",
        "@faker-js/faker": "^9.9.0",
        "@qytera/xray-client": "^4.0.0",
        "@types/node": "^24.9.2",
        "@types/semver": "^7.7.1",
        "c8": "^10.1.3",
        "cypress": "^15.8.1",
        "cypress-on-fix": "^1.1.0",
        "dotenv": "^17.2.3",
        "eslint": "^9.39.2",
        "eslint-plugin-perfectionist": "^4.15.0",
        "eslint-plugin-tsdoc": "^0.5.0",
        "graphql": "^16.12.0",
        "graphql-tag": "^2.12.6",
        "jira.js": "^5.2.2",
        "prettier": "^3.7.3",
        "shx": "^0.4.0",
        "tsx": "^4.20.6",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.46.2"
    }
}
